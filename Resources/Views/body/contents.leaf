#set("title") {
    #(userId)
}

#set("contents") {
    <h2 id="user" class="display-5 mb-3">#(userId)</h2>
    <div id="basic_stats">
    #embed("body/contents/basic_stats")
    </div>
    <div id="concrete_data">
    #for(data in concreteDataList) {
        #embed("body/contents/concrete_data")
    }
    </div>
}

#set("stylesheet") {
    <link rel="stylesheet" type="text/css" href="/css/headline.css">
}

#embed("body/body_base")
